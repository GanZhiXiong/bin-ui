{
  "name": "bin-ui",
  "version": "1.0.0",
  "description": "基于 vue-cli3 的 UI 组件库",
  "main": "lib/wb-ui.umd.min.js",
  "keywords": [
    "bin-ui",
    "bin"
  ],
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name bin-ui --dest lib src/index.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "lodash": "^4.17.11",
    "nprogress": "^0.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "velocity-animate": "^2.0.5",
    "vue": "^2.6.6",
    "vue-router": "^3.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "highlight.js": "^9.15.6",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "indent": "off",
      "space-before-function-paren": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
